<?xml version="1.0" encoding="UTF-8"?>
<!--******************************
本文件用于table变量的属性自定义
 强烈提醒: 
 1.如果你不想自定义配置,可以删除整个文件或是其中几列, 有定义的列才读自定义配置,没有定义的读数据库.不用一一对应
 2.你可以自由添加要自定义的属性，只要Table.java有set方法，并不限下面属性
 ***********************************-->
 
<!-- 
<数据库表名 className="类名称" tableAlias="表的别名">
	<数据库列名 columnAlias="列的别名" 
	javaType="自定义javaType"
	unique="是否唯一性约束" nullable="是否可以为空" pk="是否主键,在表没有主键的情况下,可以指定一个代理主键" 
	updatable="是否可以更新" insertable="是否插入"	
	enumString="枚举值,以分号分隔,示例值:M(1,男);F(0,女) 或者是:M(男);F(女)" 
	enumClassName="如果枚举有值,生成的类名称将是这个,没有枚举值，该配置无用.示例值:Sex"
	hibernateValidatorExprssion="HibernateValidator表达式: @NotNull @Min(10) @Pattern(regex=, flag=)"
	/>
</数据库表名>
 -->
<tb_material className="Material" tableAlias="Material" >
	<M_NAME columnAlias="材料名字"
		javaType="String" 
		unique="false" nullable="true" pk="false"
		updatable="true" insertable="true"
		enumString="" enumClassName="MnameEnum"
		hibernateValidatorExprssion="@Length(max=30)"
	/>
	<M_EXIST_COUNT columnAlias="现存数量"
		javaType="Integer" 
		unique="false" nullable="true" pk="false"
		updatable="true" insertable="true"
		enumString="" enumClassName="MexistCountEnum"
		hibernateValidatorExprssion=""
	/>
	<M_MIN_COUNT columnAlias="最低数量"
		javaType="Integer" 
		unique="false" nullable="true" pk="false"
		updatable="true" insertable="true"
		enumString="" enumClassName="MminCountEnum"
		hibernateValidatorExprssion=""
	/>
	<M_CATEGORY columnAlias="材料类别"
		javaType="Integer" 
		unique="false" nullable="true" pk="false"
		updatable="true" insertable="true"
		enumString="" enumClassName="McategoryEnum"
		hibernateValidatorExprssion=""
	/>
	<M_DIAMETER columnAlias="材料直径"
		javaType="Double" 
		unique="false" nullable="true" pk="false"
		updatable="true" insertable="true"
		enumString="" enumClassName="MdiameterEnum"
		hibernateValidatorExprssion=""
	/>
	<M_LENGTH columnAlias="材料长度"
		javaType="Double" 
		unique="false" nullable="true" pk="false"
		updatable="true" insertable="true"
		enumString="" enumClassName="MlengthEnum"
		hibernateValidatorExprssion=""
	/>
	<M_WEIGHT columnAlias="材料重量"
		javaType="Double" 
		unique="false" nullable="true" pk="false"
		updatable="true" insertable="true"
		enumString="" enumClassName="MweightEnum"
		hibernateValidatorExprssion=""
	/>
	<STATE columnAlias="状态"
		javaType="String" 
		unique="false" nullable="true" pk="false"
		updatable="true" insertable="true"
		enumString="" enumClassName="StateEnum"
		hibernateValidatorExprssion="@Length(max=2)"
	/>
</tb_material>
